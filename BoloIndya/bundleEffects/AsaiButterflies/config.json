{
  "name": "AsaiButterflies",
  "frx": {
    "type" : "3d_anim",
    "3d_anim" : {
      "type" : "vfx"
    },
    "media": {
      "type": "VIDEO_AUDIO",
      "file": "butall.mp4"
    }
  },
  "background": {                                                   
  "kernelSize": 5,                               
  "blurType": "big",                                

  "type": "MEDIA",

    "media": {
      "type": "VIDEO_AUDIO",
      "file": "bg.mp4",
      "videoSpeed": 1.0,
      "opacity": 1.0,
      "blend": "HARD_LIGHT",
      "mode": "RGBA"
      }
    },
  "camera": {
      "type": "MEDIA",
      "showWithFaceOnly": "true",
      "media": {
          "type": "PICTURE",
          "file": "cam.png",
          "opacity": 0.7,
          "blend": "HARD_LIGHT"
        }
      }, 
  "foreground": {
      "type": "MEDIA",
      "showWithFaceOnly": "true",
      "media": {
          "type": "PICTURE",
          "file": "cam.png",
          "opacity": 0.2,
          "blend": "OVERLAY"
        }
      }, 
  "recordDuration" : 15,
  "sounds" : [ {"trackName" : "ButterflyBGM.ogg" } ]
}